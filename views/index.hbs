<h1>{{title}}</h1>

<table class="table" border="1">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Date of Birth</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <td><a href="#">{{this.first_name}}</a></td>
      <td>{{this.last_name}}</td>
      <td>{{this.date_of_birth}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
